#Backus-naur form

<COMMAND> ::= <TERMINAL> <FLAG> <ARGUMENT> <SPECIAL> <PIPE> |
		<SPECIAL> <STRING> <PIPE>

<TERMINAL> ::= <TERMINAL> | null

<FLAG> ::= <FLAG> <FLAG> | null

<ARGUMENT> ::= <STRING> <FLAG> <ARGUMENT> | null

<SPECIAL> ::= <STRING> <SPECIAL> | null

<PIPE> ::= <COMMAND> | null

<STRING> ::= null

#automaton states:

0. std \ nothing	OK
1. new command		OK
2. pipe to command	OK
3. pipe to empty	OK
4. invalid here doc	OK
5. heredoc		OK
6. input redirection	OK
7. output redirection	OK
8. append		OK
