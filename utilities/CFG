#Backus-naur form

<COMMAND> ::= <TERMINAL> <FLAG> <ARGUMENT> <SPECIAL> <PIPE> |
		<SPECIAL> <STRING> <PIPE>

<TERMINAL> ::= <TERMINAL> | null

<FLAG> ::= <FLAG> <FLAG> | null

<ARGUMENT> ::= <STRING> <FLAG> <ARGUMENT> | null

<SPECIAL> ::= <STRING> <SPECIAL> | null

<PIPE> ::= <COMMAND> | null

<STRING> ::= null

#automaton states:

0. do nothing
1. new command
2. add input to command
3. pipe
4. heredoc
5. input redirection
6. output redirection
7. append
