#Backus-naur form

<COMMAND> ::= <TERMINAL> <FLAG> <ARGUMENT> <SPECIAL> <PIPE> |
		<SPECIAL> <STRING> <PIPE>

<TERMINAL> ::= <TERMINAL> | null

<FLAG> ::= <FLAG> <FLAG> | null

<ARGUMENT> ::= <STRING> <FLAG> <ARGUMENT> | null

<SPECIAL> ::= <STRING> <SPECIAL> | null

<PIPE> ::= <COMMAND> | null

<STRING> ::= null

#automaton states:

0. do nothing
1. new command
2. pipe to command
3. pipe to empty
4. invalid here doc
5. heredoc
6. input redirection
7. output redirection
8. append
